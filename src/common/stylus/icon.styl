    url('../fonts/city-icon.woff?jgrl2n') format('woff'),
    url('../fonts/city-icon.svg?jgrl2n#city-icon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
